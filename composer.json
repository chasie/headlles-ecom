{
    "name": "chasie/headles-ecom",
    "description": "Packaged provide Headless E-commerce for fast develop with Laravel",
    "keywords": [
        "chasie",
        "headles-ecom"
    ],
    "homepage": "https://github.com/chasie/headles-ecom",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Danil Tonkikh",
            "email": "danil4863@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Chasie\\HeadlesEcom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chasie\\HeadlesEcom\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chasie\\HeadlesEcom\\HeadlesEcomServiceProvider"
            ],
            "aliases": {
                "HeadlesEcom": "Chasie\\HeadlesEcom\\HeadlesEcomFacade"
            }
        }
    }
}
